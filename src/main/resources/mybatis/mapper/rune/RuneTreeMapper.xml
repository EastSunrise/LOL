<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wsg.lol.dao.mybatis.mapper.rune.RuneTreeMapper">
    <delete id="clear">
        delete
        from lol.r_tree
    </delete>
    <insert id="batchInsert" parameterType="wsg.lol.common.pojo.dto.rune.RuneTreeDto">
        insert into lol.r_tree
        values
        <foreach collection="list" item="item" separator=",">
            (#{item.id},
            #{item.key},
            #{item.name},
            #{item.icon})
        </foreach>
    </insert>
</mapper>